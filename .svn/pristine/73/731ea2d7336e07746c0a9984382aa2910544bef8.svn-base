

.print-wrapper { 
    margin: 0 auto;
    background: white;
    width: 100% !important;
 
    section#request-title {
        // display: none;
        flex-direction: column;
        justify-content: center;
        width: 100%;
        line-height: 18px;
        flex-wrap: wrap;
        align-content: center;
        align-items: center;

        h1, h4 {margin:0 !important}

        h3 {    
            text-align: center;    
        }

        .req-no span:first-child { vertical-align: text-top;}
        .req-no span:last-child { vertical-align: text-top; color: #b63636; padding-left: 5px;}

        .req-no { 
            text-align: right;
            width: 100px; 
        }
    } 
    section#rto-table {
        
        // max-height: 396px;
        // height: 396px;
        //max-height: 440px;
       // height: 440px;

        table {
            width: 100%;
            font-size: 0.9em;
            text-align: center;
            margin: 10px 0;
            border-spacing: 0px; 
            border-width: 0 1px 1px 0;
            border-style: solid;
            border-color: #9e9e9e;
        }

        td, th {
            border-width: 1px 0px 0px 1px;
            border-style: solid;
            border-color: #9e9e9e;
            border-spacing: 0 !important;
            overflow: hidden; 
            box-sizing: border-box;
            line-height: 16px;
        }
        
        td:empty {
            padding: 8px;
            // height: 17px !important;
        }

        td:nth-of-type(1) { 
            width: 150px;
        }

        td.td-supplier { 
            // display: -webkit-box;
            // overflow: hidden;
            // text-overflow: ellipsis;
            // -webkit-box-orient: vertical; 
            // -webkit-line-clamp: 2;
            // line-clamp: 2;
            width: 183px;
            height: 35px;
            // white-space: nowrap;
        }  
        td.td-description, td.remarks { 
            //display: -webkit-box;
            overflow: hidden;
            text-overflow: ellipsis;
            -webkit-box-orient: vertical; 
            -webkit-line-clamp: 2;
            line-clamp: 2;
            width: 183px;
            height: 35px; 
        } 
        
        tbody > tr:nth-of-type(odd) {
            background: aliceblue;
        }
        tbody > tr, tbody > td {
            height: 35px !important;
        }
        thead > tr {
            background: #3f51b5;
            color: white;
        }
    
        td {
            font-size: 0.9em;
            vertical-align: top; 
            height: 35px;
            overflow: hidden;
        } 
    }

    section#rto-approvers {
        display: flex;
        flex-direction: row;
        flex-wrap: wrap;
        justify-content: space-between;
        width: 100%;

        div { 
            display: flex;
            flex-direction: column;
            flex-wrap: nowrap;
            align-items: flex-start;
            justify-content: flex-start;
            width: 30%; 
        }
    }
        
    
    .sign{
        height: 25px;
    }
}
  
@media screen and (min-width: 800px) {
    .print-preview-request {
        border: 1px solid #e0e0e0 !important;
        padding: 20px !important;
        border-radius: 10px !important;
    }
} 

@media print {
    .loading {display: none;} 

    .print-wrapper { 
        margin: unset !important;
        left:0 !important;

        section#company-details,
        section#request-title {
            display:  flex !important;
        }
         
        thead > tr {
            background: unset !important;
            color: unset !important;
        }
        .sign {
            border-bottom: 1px solid #e0e0e0;
            width: 100%;
            padding-left: 10px; 
            height: 25px;
          }
        
    }
    tr {
        background: unset !important;
    }
}