.print-wrapper {
    #company-details { 
       h3 {
        flex: calc(100% - 100px);
        text-align: center;
        margin-left: -100px !important;
       }
    }
    h4, h3 {
        padding: 0 !important;
        margin: 0 !important;
    } 
    .border {
        border: 1px solid rgb(166, 166, 166) !important;
        line-height: 20px;
        text-align: center;
    }
    span.bottom-line { 
        border-bottom: 1px solid rgb(166, 166, 166) !important;
        width: 80%;
    }
    .logo-img {
        width: 100px;
        height: 45px;
        background: url('/assets/img/megasaver-logo_print.png') left no-repeat;
        background-size: contain;
    }
    // section {
    //     margin-bottom:10px
    // }
    td > mat-checkbox {
        width: 33%;
        text-align: center;
    }
    table {
        width: 100%;
        table-layout: fixed;
    } 
    #formitems {
        th {
            width: 120px;
            font-size: .8em;
            font-weight: 500;
        }
    }
        // .items {
        //     max-height: 396px;
        //     height: 396px;
        // }
        
        table#rfa-tb {
            width: 100%;
            font-size: 0.9em;
            text-align: center;
            margin: 0;
            border-spacing: 0px; 
            border-width: 0 1px 1px 0;
            border-style: solid;
            border-color: #9e9e9e;
        

        td, th {
            border-width: 1px 0px 0px 1px;
            border-style: solid;
            border-color: #9e9e9e;
            border-spacing: 0 !important;
            overflow: hidden; 
            box-sizing: border-box;
            line-height: 16px;
        }
        
        td:empty {
            padding: 8px;
        } 

        // td:nth-of-type(1) { 
        //     width: 150px;
        // }

        td:nth-of-type(1), td:nth-of-type(2) { 
            // display: -webkit-box;
            overflow: hidden;
            text-overflow: ellipsis;
            -webkit-box-orient: vertical; 
            -webkit-line-clamp: 1;
            line-clamp: 1;
            width: 200px; 
            height: 35px;
        }     
            
        tbody > tr:nth-of-type(odd) {
            background: aliceblue;
        }
        thead > tr:not(.no-highlight), tr.highlight {
            background: #3f51b5;
            color: white;
        }
        td, tr.highlight th, tr.no-highlight th {
            text-align: left; 
            padding-left:10px
        }
        td {
            font-size: 0.9em;
            vertical-align: top; 
        } 
        h4 {
            margin:0 !important;
        }
        .textarea {
            height: 80px
        }
    }
    table, th, td {
        border: 1px solid rgb(166, 166, 166);
        border-collapse: collapse;
        vertical-align: top;
      }
      #rfp-tb {
        tbody > tr:first-of-type {
            vertical-align: top;
            height: 100px;
        }
      }
      .clone {display: none;}
      .approvers {
         table tr:last-child {
            font-size: .8em;
            text-align: center;
            font-weight: 500;
         }
      }
}
::ng-deep .mat-checkbox-disabled .mat-checkbox-label {
    color: #212529 !important;
}

::ng-deep .mat-checkbox-disabled.mat-checkbox-checked .mat-checkbox-background, .mat-checkbox-disabled.mat-checkbox-indeterminate .mat-checkbox-background {
    background-color: #3f51b5  !important;
}
 
@media print {
.loading {display: none;} 
.print-wrapper { 
    margin: unset !important;
    left:0 !important;
    &:not(.clone) > #copy-admin {visibility: visible !important;}
    &:not(.clone) > #copy-requesitioner {visibility: hidden !important;} 
    &.clone > #copy-admin {visibility: hidden !important;}
    &.clone > #copy-requesitioner {visibility: visible !important;} 
    &.clone:nth-of-type(n+3) {
        display: none !important;
    }

    section#company-details,
    section#request-title {
        display:  flex !important;
        text-align: center;
        justify-content: center;
    }
    
    thead > tr {
        background: unset !important;
        color: unset !important;
    }
    
    .clone {display: block;}
} 
} 