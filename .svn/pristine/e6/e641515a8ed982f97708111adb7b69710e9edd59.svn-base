@media print {
.loading {display: none;} 
.print-wrapper { 
    margin: unset !important;
    left:0 !important;

    section:last-child {
      margin-bottom: 20px;  
    }

    section#company-details,
    section#request-title {
        display:  flex !important;
        text-align: center;
        justify-content: center;
    }
    
    thead > tr {
        background: unset !important;
        color: unset !important;
    }
    
    .clone {display: block;}
} 
} 