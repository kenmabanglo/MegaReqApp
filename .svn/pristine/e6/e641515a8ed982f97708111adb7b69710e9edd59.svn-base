.print-wrapper {

    h4, h3 {
        padding: 0 !important;
        margin: 0 !important;
    } 
    .border {
        border: 1px solid rgb(166, 166, 166) !important;
        line-height: 20px;
        text-align: center;
    }
    span.bottom-line { 
        border-bottom: 1px solid rgb(166, 166, 166) !important;
        width: 80%;
    }
    .logo-img {
        width: 80px;
        height: 45px;
        background: url('/assets/img/megasaver-logo_print.png') left no-repeat;
        background-size: contain;
    }
   
    table {
        width: 100%;
    }
    table, th, td {
        border: 1px solid rgb(166, 166, 166);
        border-collapse: collapse;
      }
      #rfp-tb {
        tbody > tr:first-of-type {
            vertical-align: top;
            height: 100px;
        }
      }
      .pre-wrap {
        white-space: pre-wrap;
        height: 100px;
        overflow: hidden;
        display: block;
      }
      
      .clone {display: none;}
      .payableTo {
        max-width: 600px;
        width: 600px;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
        box-sizing: border-box;
      }
}

 
