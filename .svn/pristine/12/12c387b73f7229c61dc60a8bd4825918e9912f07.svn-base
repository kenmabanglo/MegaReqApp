.print-wrapper {
    margin: 0 auto;
    background: white; 
    display: flex;
    flex-direction: column;

    h4, h3 {
        padding: 0 !important;
        margin: 0 !important;
    } 
    section#rfb-header {
        display: flex; 
        div {
            width: 49%;
            flex: 49%;
            display: flex;
        } 
    } 
    span.bottom-line { 
        width: 80%;
        border-bottom: 1px solid #e0e0e0;
    } 
 
    section#request-title {
        display: none; 
        flex-direction: column; 
        justify-content: center; 
        width: 100%;
        line-height: 18px;

        h3 {    text-align: center;    }

        .req-no span:first-child { vertical-align: text-top;}
        .req-no span:last-child { vertical-align: text-top; color: #b63636; padding-left: 5px;}

        .req-no {
            margin-top: -1.2em;
            text-align: right;
        }
    }
     
    section#rfb-table {
        width: 100%;
        max-height: 396px;
        height: 396px;

        table { 
            width: 100%;
            font-size: 0.9em;
            text-align: center;
            margin: 10px 0;
            border-spacing: 0px; 
            border-width: 0 1px 1px 0;
            border-style: solid;
            border-color: #9e9e9e;
        }

        td, th {
            border-width: 1px 0px 0px 1px;
            border-style: solid;
            border-color: #9e9e9e;
            border-spacing: 0 !important;
            overflow: hidden; 
            box-sizing: border-box;
            line-height: 16px;
        }
        
        td:empty {
            padding: 8px;
        }

        td:first-child, th:first-child {
            width: 40px; 
        }

        th:nth-of-type(2), td:nth-of-type(2) { 
            width: 183px; 
        }    
        td:not(:nth-of-type(1)) { 
            overflow: hidden;
            text-overflow: ellipsis;
            -webkit-line-clamp: 2;
            line-clamp: 2;
            -webkit-box-orient: vertical; 
        } 
        th:nth-of-type(3), td:nth-of-type(3),
        th:last-of-type, td:last-of-type  { 
            width: 98.5px; 
        }     
         
        tbody > tr:nth-of-type(odd) {
            background: aliceblue;
        }
        thead > tr {
            background: #3f51b5;
            color: white;
        }
 
        td {
            font-size: 0.9em;
            vertical-align: top; 
            height: 35px;
            max-height: 35px;
        } 
    }

    section#rfb-footer1 {
        display: flex;
        flex-wrap: wrap;
        width: 100%;
        align-content: flex-start;
        justify-content: flex-start;        
    }

    section#rfb-footer2 {
        display: flex;
        flex-wrap: nowrap;
        flex-direction: row; 
        margin: 0 auto;
        width: 100%;
        justify-content: space-around;
 
        h4 {
            margin: 0 !important;
            margin-top: 0.5em !important;
            width: 80%; 
            border-top: 1px solid #e0e0e0;
        }
        div {
            flex: 33%;
        }
        span { 
            width: 100%; 
            padding-left: 10px; 
        }
    } 
}
 
 
@media print {
.loading {display: none;} 
.print-wrapper { 
    margin: unset !important;
    left:0 !important;
    width:100%;
    
    section#company-details,
    section#request-title {
        display:  flex !important;
    }

    section#rfb-table {
        width: 100%;
    }
    
    section#rfb-table td:nth-of-type(2) { 
        width: calc(100% - 100px) !important;
        overflow: hidden;
        text-overflow: ellipsis; 
        -webkit-line-clamp: 2;
        line-clamp: 2;
        -webkit-box-orient: vertical;
    }

    thead > tr {
        background: unset !important;
        color: unset !important;
    }
    
    
}
tr {
    background: unset !important;
}
} 